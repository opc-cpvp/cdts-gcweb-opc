			</div>

			{if $siteMenu != false}
			<nav id="wb-sm" data-ajax-replace='{call .base data="all" /}ajax/sitemenu-en' data-trgt="mb-pnl" class="wb-menu visible-md visible-lg" typeof="SiteNavigationElement" aria-labelledby="topics-menu-section">
